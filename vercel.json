{
  "version": 3,
  "builds": [
    {
      "src": "api/diagnose.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/diagnose"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}